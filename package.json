{
	"name": "change-case",
	"version": "1.0.3",
	"description": "Plugin to let you change the case (UPPER CASE, camelCase, snake_case, etc) of the current selection.",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json",
		"semantic-release": "semantic-release",
		"test": "node --loader tsx --test ./test/index.ts"
	},
	"keywords": [],
	"author": "David Brockman",
	"license": "MIT",
	"dependencies": {
		"change-case": "4.1.2",
		"lower-case": "2.0.2",
		"upper-case": "2.0.2"
	},
	"devDependencies": {
		"@semantic-release/git": "^10.0.1",
		"@types/node": "^20.6.0",
		"builtin-modules": "^3.3.0",
		"cz-conventional-changelog": "^3.3.0",
		"esbuild": "^0.19.2",
		"obsidian": "^1.4.4",
		"semantic-release": "^21.1.1",
		"tslib": "^2.6.2",
		"tsx": "^3.12.8",
		"typescript": "^5.2.2"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/dbrockman/obsidian-change-case.git"
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	}
}
